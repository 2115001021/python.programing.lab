num = int(input('enter any integer number :'))
count = 0
if num == 1:
    print(f'the given number {num} is not prime')
elif num > 1:
    for i in range (2,num):
        if num % i == 0:
            count+=1 
    if count:
        print(f'the given number {num} is not prime')
    else:
        print(f'the given number {num} is prime')
        
